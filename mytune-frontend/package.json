{
  "name": "mytune-frontend",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://finnvandenheuvel.github.io/MyTune-Frontend/",
  "dependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.0",
    "axios": "^1.13.2",
    "bootstrap": "^5.3.8",
    "prop-types": "^15.8.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "react-scripts build",
    "format": "prettier --write .",
